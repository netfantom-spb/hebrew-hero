{
  "name": "hebrew-hero",
  "version": "0.0.1",
  "description": "\"Hebrew Hero\" is a Chrome extension designed to assist Russian-speaking users in navigating Hebrew-language websites.",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prerelease": "del build -R /Q",
    "release": "cross-env NODE_ENV=production rollup -c",
    "testsrv": "npx http-server tests/testpage -p 9000"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com.netfantom:netfantom-spb/hebrew-hero.git"
  },
  "browserslist": [
    "last 3 Chrome version"
  ],
  "author": "Viktor Egorov",
  "license": "ISC",
  "devDependencies": {
    "http-server": "^14.1.1",
    "rollup": "^3.17.2",
    "rollup-plugin-chrome-extension": "^3.6.10",
    "rollup-plugin-cleanup-dir": "^1.0.7",
    "rollup-plugin-zip": "^1.0.3"
  }
}
